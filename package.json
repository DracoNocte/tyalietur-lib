{
  "name": "tyalietur-lib",
  "version": "0.0.1",
  "description": "Common lib for Tyalietur bot and API",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "module": "dist/main.esm.js",
  "scripts": {
    "start": "ts-node src/main.ts",
    "build": "rollup -c --bundleConfigAsCjs",
    "clean": "rm -rf dist/",
