{
  "name": "tyalietur-lib",
  "version": "0.1.0",
  "description": "Common lib for Tyalietur bot and API",
  "main": "./lib/tyalietur-lib.umd.cjs",
  "module": "./lib/tyalietur-lib.mjs",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/tyalietur-lib.mjs",
      "require": "./lib/tyalietur-lib.umd.cjs"
    }
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "npm run clean && vite build",
    "clean": "rm -rf ./lib",
    "test": "jest --passWithNoTests",
    "test:cov": "jest --coverage",
    "test:ci": "jest --coverage --ci --runInBand",
    "prettier": "prettier -c src",
    "lint": "eslint --max-warnings 0 src",
    "release": "release-it",
    "release:dry": "release-it --dry-run",
    "sandbox": "ts-node -r tsconfig-paths/register  src/sandbox.ts"
  },
  "author": "Draco Nocte <dragonnight.gaming@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "discord.js": "^14.14.1"
  },
  "devDependencies": {
    "@release-it/bumper": "^6.0.1",
    "@release-it/conventional-changelog": "^8.0.1",
    "@release-it/keep-a-changelog": "^5.0.0",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "release-it": "^17.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.2",
    "type-fest": "^4.8.2",
    "typescript": "~5.2.2",
    "vite": "^5.0.3",
    "vite-plugin-dts": "^3.6.3",
    "vite-tsconfig-paths": "^4.2.1"
  }
}
